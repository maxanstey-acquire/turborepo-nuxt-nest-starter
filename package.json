{
  "name": "speechscribe-turbo",
  "private": true,
  "version": "0.0.0",
  "packageManager": "npm@11.6.1",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev --parallel",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "fix": "turbo run fix",
    "check": "turbo run check",
    "go": "npm run fix && npm run build && npm run dev",
    "check-env": "ts-node -r tsconfig-paths/register scripts/check-env.ts",
    "prepare": "turbo run prepare --parallel && npm run build --workspace packages/shared && npm run build --workspace packages/contracts",
    "test": "turbo run test",
    "nuke": "rm -rf node_modules && find . -name '.nuxt' -type d -prune -exec rm -rf {} + && find . -name 'dist' -type d -prune -exec rm -rf {} + && find . -name '.output' -type d -prune -exec rm -rf {} +"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^2.1.1",
    "@eslint/js": "^9.8.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.0",
    "@nuxt/devtools": "^3.1.0",
    "@nuxt/eslint": "^1.10.0",
    "@nuxt/eslint-config": "~0.5.6",
    "@nuxt/fonts": "^0.12.1",
    "@nuxt/icon": "^2.1.0",
    "@nuxt/image": "^2.0.0",
    "@nuxt/kit": "^4.2.1",
    "@nuxt/ui": "^4.1.0",
    "@nuxt/ui-templates": "^1.3.1",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "^1.15.2",
    "@swc/helpers": "~0.5.11",
    "@tailwindcss/postcss": "^4.1.17",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "20.19.9",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/parser": "^8.40.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/coverage-v8": "^3.0.5",
    "@vitest/ui": "^3.0.0",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^10.0.0",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-boundaries": "^5.1.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.2.4",
    "globals": "^16.0.0",
    "h3": "^1.8.2",
    "jest": "^30.0.2",
    "jest-environment-node": "^30.0.2",
    "jest-util": "^30.0.2",
    "jiti": "2.4.2",
    "jsdom": "~22.1.0",
    "jsonc-eslint-parser": "^2.1.0",
    "nuxt": "^4.2.1",
    "prettier": "^3.6.2",
    "sass": "^1.94.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.4.0",
    "ts-loader": "^9.5.2",
    "ts-node": "10.9.1",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.3.0",
    "tsup": "^8.3.0",
    "turbo": "^2.1.0",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.40.0",
    "vite": "^7.0.0",
    "vite-tsconfig-paths": "^5.1.1",
    "vitest": "^3.0.0",
    "vue-tsc": "^2.2.8"
  },
  "dependencies": {
    "@azure/identity": "^4.13.0",
    "@azure/openai": "^2.0.0",
    "@azure/storage-blob": "^12.28.0",
    "@dotenvx/dotenvx": "^1.51.0",
    "@microsoft/microsoft-graph-client": "^3.0.7",
    "@microsoft/microsoft-graph-types": "^2.43.1",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/platform-express": "^11.0.1",
    "axios": "^1.6.0",
    "openai": "^6.6.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "vue": "^3.5.24",
    "vue-router": "^4.6.3",
    "zod": "^3.23.8"
  }
}
