{
  "name": "app-turbo",
  "private": true,
  "version": "0.0.0",
  "packageManager": "pnpm@9.12.3",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev --parallel",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "fix": "turbo run fix",
    "check": "turbo run check",
    "go": "pnpm run prepare && pnpm run fix && pnpm run build && pnpm run dev",
    "check-env": "ts-node -r tsconfig-paths/register scripts/check-env.ts",
    "prepare": "turbo run prepare --parallel && pnpm --filter @app/shared run build && pnpm --filter @app/contracts run build",
    "test": "turbo run test",
    "prisma": "pnpm --filter @app/api... prisma",
    "db:migrate": "pnpm --filter @app/prisma run db:migrate",
    "db:deploy": "pnpm --filter @app/prisma run db:deploy",
    "db:studio": "pnpm --filter @app/prisma run db:studio",
    "nuke": "rm -rf node_modules && find . -name '.nuxt' -type d -prune -exec rm -rf {} + && find . -name 'dist' -type d -prune -exec rm -rf {} + && find . -name '.output' -type d -prune -exec rm -rf {} +"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^2.1.4",
    "@eslint/js": "^9.39.1",
    "@nestjs/cli": "^11.0.12",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.9",
    "@nuxt/devtools": "^3.1.0",
    "@nuxt/eslint": "^1.10.0",
    "@nuxt/eslint-config": "~0.5.7",
    "@nuxt/fonts": "^0.12.1",
    "@nuxt/icon": "^2.1.0",
    "@nuxt/image": "^2.0.0",
    "@nuxt/kit": "^4.2.1",
    "@nuxt/ui": "^4.2.1",
    "@nuxt/ui-templates": "^1.3.4",
    "@swc-node/register": "~1.9.2",
    "@swc/core": "^1.15.3",
    "@swc/helpers": "~0.5.17",
    "@tailwindcss/postcss": "^4.1.17",
    "@types/express": "^5.0.5",
    "@types/jest": "^30.0.0",
    "@types/node": "20.19.9",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/parser": "^8.47.0",
    "@vitejs/plugin-vue": "^6.0.2",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-boundaries": "^5.3.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.5.0",
    "h3": "^1.15.4",
    "jest": "^30.2.0",
    "jest-environment-node": "^30.2.0",
    "jest-util": "^30.2.0",
    "jiti": "2.4.2",
    "jsdom": "~22.1.0",
    "jsonc-eslint-parser": "^2.4.1",
    "nuxt": "^4.2.1",
    "prettier": "^3.6.2",
    "sass": "^1.94.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "ts-loader": "^9.5.4",
    "ts-node": "10.9.1",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.8.1",
    "tsup": "^8.5.1",
    "turbo": "^2.6.1",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.47.0",
    "vite": "^7.2.4",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4",
    "vue-tsc": "^2.2.12"
  },
  "dependencies": {
    "@azure/identity": "^4.13.0",
    "@azure/openai": "^2.0.0",
    "@azure/storage-blob": "^12.29.1",
    "@dotenvx/dotenvx": "^1.51.1",
    "@microsoft/microsoft-graph-client": "^3.0.7",
    "@microsoft/microsoft-graph-types": "^2.43.1",
    "@nestjs/common": "^11.1.9",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.9",
    "@nestjs/platform-express": "^11.1.9",
    "axios": "^1.13.2",
    "openai": "^6.9.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "vue": "^3.5.24",
    "vue-router": "^4.6.3",
    "zod": "^3.25.76"
  }
}
