{
  "name": "@speechscribe/contracts",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "exports": {
    "./client": {
      "types": "./dist/client.d.ts",
      "import": "./dist/client.js",
      "default": "./dist/client.js"
    },
    "./contract": {
      "types": "./dist/contract.d.ts",
      "import": "./dist/contract.js",
      "default": "./dist/contract.js"
    },
    "./responses/me": {
      "types": "./dist/responses/me.d.ts",
      "import": "./dist/responses/me.js",
      "default": "./dist/responses/me.js"
    },
    "./schemas/user": {
      "types": "./dist/schemas/user.d.ts",
      "import": "./dist/schemas/user.js",
      "default": "./dist/schemas/user.js"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.json && tsc-alias",
    "check": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "@orpc/client": "^1.11.3",
    "@orpc/contract": "^1.11.3",
    "@orpc/openapi-client": "^1.11.3",
    "@speechscribe/shared": "workspace:*",
    "zod": "^3.23.8"
  },
  "turbo": {
    "outputs": ["dist/**"]
  }
}
