{
  "name": "@app/shared",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "exports": {
    "./types/user": {
      "types": "./dist/types/user.d.ts",
      "import": "./dist/types/user.js",
      "default": "./dist/types/user.js"
    },
    "./types/feature-flags": {
      "types": "./dist/types/feature-flags.d.ts",
      "import": "./dist/types/feature-flags.js",
      "default": "./dist/types/feature-flags.js"
    },
    "./types/subscription": {
      "types": "./dist/types/subscription.d.ts",
      "import": "./dist/types/subscription.js",
      "default": "./dist/types/subscription.js"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.json && tsc-alias",
    "check": "tsc -p tsconfig.json --noEmit",
    "clean": "rm -rf dist"
  },
  "turbo": {
    "outputs": ["dist/**"]
  }
}
